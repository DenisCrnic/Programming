import java.util.Scanner;
public class Naloga12 {
	public static void main(String[] args){
		Scanner input = new Scanner(System.in);
		StringBuffer b = new StringBuffer(input.nextLine()).insert(0, ' ');
		for(int i = 0; i < b.length(); i++){
			if(b.charAt(i)==' ')
				b.setCharAt(i+1, 'x');
		}
	}
}
