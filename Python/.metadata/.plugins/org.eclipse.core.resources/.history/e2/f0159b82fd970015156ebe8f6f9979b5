import random
from Crypto.Random.random import randrange
while(True):
    try:
        x = input("Vnos številke: ")
        if(type(x) == int):
            break
        else:
            print "Vnos mora biti celo število"
    except SyntaxError:
        print "Polje nesme biti prazno"
    except NameError:
        print "Vnos mora biti številka"

