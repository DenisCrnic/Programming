#-*-encoding: utf-8-*-

from numpy.core.defchararray import lower

while(True):
    a = raw_input("vnesite besedo: ")
    if(len(a) == 0):
        print "vnos ne sme biti prazen!"
    else:
        break

if(lowerr(a) == lower(a[::-1])):
    print "beseda JE palindrom!"
else:
    print "beseda NI palindrom!"