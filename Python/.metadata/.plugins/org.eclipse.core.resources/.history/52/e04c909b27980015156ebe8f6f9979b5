#-*- coding: utf-8 -*-

def enkripcija(a, key):
    x = []
    for i in a:
        x.append(ord(i) + key))
    return x
while True:
    a = raw_input("Vnesite besedilo: ")
    while True:
        try:
            b = input("Vnesite kljuc: ")
            break
        except:
            print "vnos mora biti stevilo"
    print enkripcija(a, b)