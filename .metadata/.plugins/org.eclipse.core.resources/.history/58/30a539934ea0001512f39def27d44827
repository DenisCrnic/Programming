#! /usr/bin/python
#-*-encoding: utf-8-*-

class Human:
    
    #Attributes
    name = ""
    age = 0
    height = 0
    
    #set Functions
    def setName(self, a):
        name = a
        
    def setAge(self, a):
        age = a
        
    def setHeight(self, a):
        height = a
    
    #get Functions
    def getName(self):
        return name
        
    def getAge(self):
        return age
        
    def getHeight(self):
        return height
    
def Main():
    denis = Human()
    denis.setName()
    """denis.setAge()
    denis.setHeight()
    
    print denis.getName()
    print denis.getAge()
    print denis.getHeight()"""
    denis.name = "Denis"
    print denis.name
if __name__ == '__main__':
    Main()