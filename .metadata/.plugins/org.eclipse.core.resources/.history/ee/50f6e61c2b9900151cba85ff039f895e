#-*- encoding: utf-8 -*-
from _collections import deque

def print_array(a):
    for i in a:
        print "-",i


to_do_list = deque([])
run = True
while run:
    print_array(to_do_list)
    select = raw_input("""\
TO DO!
a - add task
d - remove last task
""")
    if (select == "a"):
        task = raw_input("to do: ")
        if(len(task) != 0):
            to_do_list.appendleft(task)
        else:
            print "vpi≈°ite task"
    elif (select == "d"):
        to_do_list.pop()
    else:
        print "wrong selection"
    